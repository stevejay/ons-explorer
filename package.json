{
  "name": "ons-explorer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:dockerfile": "dockerfilelint Dockerfile*",
    "lint:prettier": "prettier --loglevel warn --check .",
    "lint:eslint": "eslint --ext .html,.js,.jsx,.ts,.tsx --max-warnings 0 .",
    "lint:ts": "tsc --noEmit --pretty",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:md": "markdownlint --ignore-path ./.gitignore .",
    "lint": "npm-run-all --parallel --continue-on-error 'lint:*'",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "preview:storybook": "serve ./storybook-static --listen 5000",
    "test:e2e": "playwright test --workers=1",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage,",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@mdx-js/react": "^1.6.22",
    "@playwright/test": "^1.18.0",
    "@storybook/addon-actions": "^6.5.0-alpha.22",
    "@storybook/addon-docs": "^6.5.0-alpha.22",
    "@storybook/addon-essentials": "^6.5.0-alpha.22",
    "@storybook/addon-links": "^6.5.0-alpha.22",
    "@storybook/react": "^6.5.0-alpha.22",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7",
    "@types/chai": "^4.3.0",
    "@types/chai-dom": "^0.0.12",
    "@types/eslint": "^8",
    "@types/jsdom": "^16.2.14",
    "@types/mdx-js__react": "^1",
    "@types/node": "^17.0.10",
    "@types/prettier": "^2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "chai-dom": "^1.11.0",
    "dockerfilelint": "^1.8.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-testing-library": "^5.0.4",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.1",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "storybook-builder-vite": "^0.1.13",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "supports-color": "^9.2.1",
    "tailwindcss": "^3.0.15",
    "tsc-files": "^1.1.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.2.0"
  },
  "packageManager": "yarn@3.1.1",
